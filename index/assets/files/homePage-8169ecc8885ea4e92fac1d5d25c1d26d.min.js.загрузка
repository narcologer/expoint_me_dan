firstOpen=false;}
function onReady(e){e.target.playVideo();firstOpen=false;}
if(firstOpen===false&&player){player.playVideo();}
sendGa4('popup_tracking','click_attempt','videoguide');return true;});modalYoutube.addEventListener('hidden.bs.modal',function(){player.stopVideo();});document.addEventListener('click',function(event){if(!event.target.closest('.modal-dialog')&&document.querySelector('#modalYoutube.show')){let modal=document.querySelector('#modalYoutube.show');let modalInstance=bootstrap.Modal.getInstance(modal);modalInstance.hide();}});});}catch(e){console.log(e);}
try{var not_block_utp,passiveSupported;(function($){function handleButtonOnClick(){if(not_block_utp!=='1'){not_block_utp=1;document.cookie="not_block_utp=1; path=/; max-age=7776000";const els=document.getElementsByTagName('body');if(undefined!==els&&not_block_utp!=''){els[0].classList.add('not_block_utp');}}}
